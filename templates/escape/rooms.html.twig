    <!--Carousel de toutes les rooms -->
    <div id="carouselEscape" class="carousel slide carousel-fade carousel_dooz" data-bs-ride="carousel">
        <div class="carousel-indicators">
            {% for room in rooms %}
                <button type="button" data-bs-target="#carouselEscape" data-bs-slide-to="{{ loop.index0 }}" class="{% if loop.first %}active{% endif %}"
                        aria-current="true" aria-label="Slide {{ loop.index }}"></button>
            {% endfor %}
        </div>
        <div class="carousel-inner">
            <div class="filter_carousel">
                <div class="filter_carousel_1"></div>
                <div class="filter_carousel_2"></div>
            </div>
            {% for room in rooms %}
                <div class="carousel-item {% if loop.first %}active{% endif %}">
                    <img src="{{ room.banner|bannerRoom }}" class="d-block w-100" alt="{{ room.name }}">
                    <div class="carousel-caption">
                        {% if app.request.locale == 'fr' %}
                            <h2 class="link_underline"><a href="{{ path('room', {slug: room.slug}) }}">{{ room.name }}</a></h2>
                        {% elseif app.request.locale == 'en' %}
                            <h2 class="link_underline"><a href="{{ path('room', {slug: room.slug}) }}">{{ room.nameEn }}</a></h2>
                        {% endif %}
                    </div>
                </div>
            {% endfor %}
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselEscape" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselEscape" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>

