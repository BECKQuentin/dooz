    </head>

        {% if isAdminPage is not defined %}

            <!--Header-->
            {% if isHomepage is not defined %}
                {% include 'base/header.html.twig' %}
            {% endif %}

            <body>
                {% if isHomepage is not defined %}
                    <div id="content">
                        {% block body %}

                        {% endblock %}
                    </div>
                {% endif %}
                {% if isHomepage is defined %}
                    <div id="homepage">
                        {% block homepage %}

                        {% endblock %}
                    </div>
                {% endif %}
            </body>

            <!--Footer-->
            {% if isHomepage is not defined %}
                <footer>
                    {% include 'base/footer_map.html.twig' %}
                    {% include 'base/footer_cgv.html.twig' %}
                </footer>
            {% endif %}

        {% else %}
            <div id="back_office" class="d-flex">

                <section id="back_office_menu" class="col-xl-1 col-md-2 col-sm-3 col-4">
                    {% include 'admin/tool/menu.html.twig' %}
                </section>
                <section class="offset-xl-1 offset-md-2 offset-sm-3 offset-4 col-xl-11 col-md-10 col-sm-9 col-8">
                    {% include 'admin/tool/flash.html.twig' %}

                    {% block BO %}



                    {% endblock %}
                </section>
            </div>
        {% endif %}





</html>
